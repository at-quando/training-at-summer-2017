
<div class="">
  <a class="hiddenanchor" id="toregister"></a>
  <a class="hiddenanchor" id="tologin"></a>
  <div id="wrapper">
    <div id="login" class="animate form">
      <section class="login_content">
        <%= form_for :session, url: login_path do |f| %>
          <h1>Login Form</h1>
          <div>
            <%= f.email_field :email, class: "form-control", placeholder: "Username", required: "" %>
          </div>
          <div>
          	<%= f.password_field :password, class: "form-control", placeholder: "Password", required: "" %>
          </div>
          <div>
            <%= f.submit "Log in", class: "btn btn-default submit" %>
            <%= link_to "Lost your password?", new_password_reset_path %>
          </div>
          <div class="clearfix"></div>
          <div class="separator">
            <p class="change_link">New to site?
              <%= link_to "Sign Up" ,register_path %>
            </p>
            <div class="clearfix"></div>
            <br />
            <div>
              <h1> Book Ordering!</h1>
              <p>Â©2017 All Rights Reserved. Book Booking! is a Bootstrap 3 template. Privacy and Terms</p>
            </div>
          </div>
        <% end %>
        <!-- form -->
      </section>
      <!-- content -->
    </div>
  </div>
</div>




